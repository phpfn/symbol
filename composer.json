{
    "name": "serafim/symbol",
    "type": "library",
    "description": "The Symbol type PHP implementation",
    "license": "MIT",
    "keywords": [
        "placeholder",
        "symbol",
        "functional",
        "type"
    ],
    "homepage": "https://github.com/SerafimArts/Symbol",
    "support": {
        "issues": "https://github.com/SerafimArts/Symbol/issues",
        "source": "https://github.com/SerafimArts/Symbol"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\Symbol\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\Symbol\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
