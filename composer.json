{
    "name": "phpfn/symbol",
    "type": "library",
    "description": "The Symbol type implementation",
    "license": "MIT",
    "keywords": [
        "placeholder",
        "symbol",
        "functional",
        "type"
    ],
    "homepage": "https://github.com/phpfn",
    "support": {
        "issues": "https://github.com/phpfn/phpfn/issues",
        "source": "https://github.com/phpfn/symbol"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Fun\\Symbol\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Fun\\Symbol\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
